<%@ page session="false" %>
    <c:if test="${articleCategory ne null && articleCategory.sectionid ne null}"><nav><h1><h:local key="page_sideRecentCategory"><h:param object="${articleCategory.name}"/></h:local></h1><ul><g:recentArticles number="10" section="${articleCategory}">
        <li><g:articleUrl article="${_article}"/></li></g:recentArticles>
    </ul></nav></c:if>
    <nav><h1><h:local key="page_sideRecent"/></h1><ul><g:recentArticles number="10">
        <li><g:articleUrl article="${_article}"/></li></g:recentArticles>
    </ul></nav>